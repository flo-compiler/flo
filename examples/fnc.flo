fnc map(array: int [], func: (int) => int): int [] {
    arr = [func(array[0])]
    for i = 1; i < len(array); i++{
        arr << func(array[i])
    }
    return arr
}
fnc double(x: int): int {
    return x*2
}
original =  [1, 2, 3, 4, 5]
result = map(original, double)
println(result)
