fnc fib_rec(n: int): int => if (n<3) return 1 else return fib_rec(n-1) + fib_rec(n-2)
fnc fib(n: int): int => {
    last_num = 1
    slast_num = 1
    curr_num: int = 1
    for i = 2; i < n; i+=1 {
        slast_num = last_num 
        last_num = curr_num 
        curr_num = last_num + slast_num
    }
    return curr_num
}
print("Enter a number to compute it\'s fibonnaci sum: ")
x = input()
println(fib_rec(x))