class Chef {
    specialty: string
    constructor(this, specialty: string) {
        this.specialty = specialty
    }
    print_specialty(this) {
        println(this.specialty)
    }
}
class ItalianChef(Chef) {
    constructor(this) {
        this.super.constructor("Pasta")
    }
}
fnc main(){
    let chef: Chef = new ItalianChef()
    chef.print_specialty()
}