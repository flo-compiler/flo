{
    "name": "flo",
    "displayName": "flo",
    "description": "Flo programming language",
    "version": "0.0.1",
    "publisher": "vanelk",
    "repository":  {
		"type": "git",
		"url": "https://github.com/vanelk/flo/tree/main/editors/vscode"
	},
    "engines": {
        "vscode": "^1.62.0"
    },
    "categories": [
