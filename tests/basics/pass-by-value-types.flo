fnc init_array(x: int, y: int): int[2] {
    return [x, y]
}
type Pos = {x: int, y: int}
fnc init_pos(x: int, y: int): Pos {
    return {x: x, y: y}
}
fnc change_pos(p: Pos): Pos {
    p.x++
    return p
}
fnc main(): int {
    let p = init_pos(1, 2)
    let arr = init_array(3, 5)
    p = change_pos({x: arr[0], y: p.y})
    if (p.x == 4 and p.y == 2) return 0
    return 1
}