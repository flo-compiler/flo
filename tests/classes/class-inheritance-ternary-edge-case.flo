class Base {
    constructor(this){}
}
class DerivedA (Base) {
    constructor(this){}
}
class DerivedB (Base) {
    constructor(this){}
}
fnc test(base: Base){}
fnc main(){
    let b: Base()
    let da: DerivedA()
    let db: DerivedB()
    let condition = true
    test(condition ? b : db)
    test(condition ? db : b)
    test(condition ? da : db)
}