const MAX_BUFFSIZE = 12400
class Request {
    raw_buffer: i8*
    buffer_size: int
    fnc constructor(){
        this.raw_buffer = new i8(MAX_BUFFSIZE)
    }
    fnc set_size(size: int = MAX_BUFFSIZE){
        this.buffer_size = size
    }
    fnc to_string(): string{
        return new string(this.raw_buffer, this.buffer_size)
    }
}