const MAX_BUFFSIZE = 12400
class Request {
    raw_buffer: u8*
    buffer_size: uint
    constructor(this){
        this.raw_buffer = new u8*(MAX_BUFFSIZE)
    }
    set_size(this, size = MAX_BUFFSIZE){
        this.buffer_size = size
    }
    to_string(this): string{
        return new string(this.raw_buffer, this.buffer_size)
    }
}