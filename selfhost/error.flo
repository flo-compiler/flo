class FloError {
    message: string
    range: Range
    constructor(message: string, range: Range){
        this.range = range
        this.message = message
    }
}
fnc print_errors(filename: string, errors: Array<FloError>, content: string){
    for i = 0; i < errors.length; i++ {
        println(errors[i].message)
    }
    if errors.length > 0 exit()
}